{
	"llvm-target":   "wasm32-unknown-wasi",
	"cpu":           "generic",
	"features":      "+bulk-memory,+nontrapping-fptoint,+sign-ext",
	"build-tags":    ["tinygo.wasm", "wasi", "runtime_memhash_leveldb"],
	"goos":          "linux",
	"goarch":        "arm",
	"linker":        "wasm-ld",
	"libc":          "wasi-libc",
	"scheduler":     "asyncify",
	"default-stack-size": 16384,
	"cflags": [
		"-mbulk-memory",
		"-mnontrapping-fptoint",
		"-msign-ext"
	],
	"ldflags": [
		"--stack-first",
		"--no-demangle",
		"/usr/local/tinygo/lib/wasi-libc/sysroot/lib/wasm32-wasi/libclang_rt.builtins-wasm32.a"
	],
	"extra-files": [
		"src/runtime/asm_tinygowasm.S"
	],
	"emulator":      "wasmtime --mapdir=/tmp::{tmpDir} {}",
	"wasm-abi":      "generic"
}
